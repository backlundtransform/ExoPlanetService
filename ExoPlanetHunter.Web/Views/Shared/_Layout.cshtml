<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

      <script src="~/lib/ckeditor/ckeditor.js"></script>
</head>
<body>
    <div>
        <h1>ExoPlanetHunter</h1>

        <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-600" src="~/img/images.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-600" src="~/img/images.jpg"  alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-600" src="~/img/images.jpg"  alt="Third slide">
    </div>
  </div>
</div>
        @RenderBody()

    </div>
    <script src="~/js/main.build.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
  
</body>
</html>