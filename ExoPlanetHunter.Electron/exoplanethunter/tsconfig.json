{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "esnext",
    "jsx": "react",
    "skipLibCheck": false,
    "moduleResolution": "node",
    "allowJs": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "target": "es5",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src"
  ]
}
